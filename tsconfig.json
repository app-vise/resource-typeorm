{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@config/appvise/resource": [
        "./src/config/resource"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}
